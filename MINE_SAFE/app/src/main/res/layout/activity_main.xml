<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.android.minesafe.MainActivity">

<com.mapbox.mapboxsdk.maps.MapView
    android:id="@+id/mapView"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:mapbox_cameraTargetLat="3.127789"
    app:mapbox_cameraTargetLng="-73.744354"
    app:mapbox_cameraZoom="12"
    app:mapbox_styleUrl="@string/mapbox_style_light"/>
    <!--app:mapbox_styleUrl="@string/mapbox_style_satellite"-->
    />

</RelativeLayout>
